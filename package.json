{
  "name": "@theaccessibleteam/env-doctor",
  "version": "1.0.1",
  "description": "Analyze and validate environment variables in your codebase. Detect missing, unused, and misconfigured env vars with framework-aware scanning.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./runtime": {
      "import": {
        "types": "./dist/runtime.d.ts",
        "default": "./dist/runtime.js"
      },
      "require": {
        "types": "./dist/runtime.d.cts",
        "default": "./dist/runtime.cjs"
      }
    }
  },
  "bin": {
    "env-doctor": "./dist/cli.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/cli.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "validate": "pnpm run typecheck && pnpm run lint && pnpm run test:run",
    "clean": "rm -rf dist coverage .tsbuildinfo",
    "prepublishOnly": "pnpm run validate && pnpm run build",
    "release": "changeset publish",
    "docs:dev": "cd docs && npm run start",
    "docs:build": "cd docs && npm run build",
    "icons:generate": "node scripts/generate-icons.js",
    "prepare": "echo 'Ready to develop!'"
  },
  "keywords": [
    "env",
    "environment",
    "variables",
    "dotenv",
    "linter",
    "validator",
    "security",
    "secrets",
    "cli",
    "nextjs",
    "vite",
    "react",
    "typescript",
    "devtools",
    "dx"
  ],
  "author": {
    "name": "The Accessible Team",
    "url": "https://github.com/WOLFIEEEE"
  },
  "license": "MIT",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/WOLFIEEEE"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WOLFIEEEE/env-doctor.git"
  },
  "bugs": {
    "url": "https://github.com/WOLFIEEEE/env-doctor/issues"
  },
  "homepage": "https://WOLFIEEEE.github.io/env-doctor",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.1",
    "@typescript-eslint/typescript-estree": "^8.18.2",
    "commander": "^12.1.0",
    "glob": "^11.0.0",
    "picocolors": "^1.1.1",
    "simple-git": "^3.27.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
